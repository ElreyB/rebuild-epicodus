<!DOCTYPE html>
<html>
  <head>
    <title>RebuildEpicodus</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container">
      <%= flash[:alert] %>
      <%= flash[:notice] %>
      <nav>
        <%= yield(:nav_lesson) %>
      </nav>
      <%= yield %>
    </div>
    <footer>
      <nav>
        <%= yield(:nav_footer) %>
      </nav>
    </footer>
  </body>
</html>
