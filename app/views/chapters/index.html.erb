

<% if @chapters.any? %>
  <ul>
    <h1>Courses</h1>
    <% @chapters.each do |chapter| %>
      <li><h3><%= link_to chapter.name, chapter_path(chapter) %> </h3></li>
    <% end %>
  </ul>
<% else %>
  <h3>Coming Soon!</h3>
<% end %>

<% content_for(:nav_footer) do %>
<li><a href="/chapters/new">New Chapter</a></li>
<% end %>
